{
  "name": "qume-monorepo",
  "version": "1.0.5",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/test/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "publish": "yarn publish:qume && yarn publish:qume-react",
    "publish:qume": "cd packages/qume && yarn publish",
    "publish:qume-react": "cd packages/qume-react && yarn publish"
  },
  "devDependencies": {
    "turbo": "^2.5.5"
  },
  "packageManager": "yarn@4.9.2",
  "repository": "git@github-qume:qumedev/qume.git",
  "author": "qumedev <queryme.dev@gmail.com>",
  "license": "Apache-2.0"
}
